{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",

    "db:init": "tsx server/scripts/dbInit.ts",
    "db:reset": "tsx server/scripts/dbInit.ts --reset",

    "dev:server": "NODE_ENV=development npm run db:init && tsx watch server/index.ts",
    "dev:worker": "tsx watch server/workers/index.ts",

    "build": "vite build && tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",

    "start:server": "NODE_ENV=production node dist/server/index.js",
    "start:worker": "node dist/server/workers/index.js",
    "start:worker:email": "WORKER_TYPE=email node dist/server/workers/index.js",
    "start:worker:order": "WORKER_TYPE=order node dist/server/workers/index.js",
    "start:worker:payment": "WORKER_TYPE=payment node dist/server/workers/index.js",

    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/bull": "^3.15.9",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "prom-client": "^15.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
